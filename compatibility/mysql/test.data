CREATE DATABASE test

USE test

CREATE TABLE tb1 (id int, value float, c1 char(10), primary key(id))

INSERT INTO tb1 VALUES (1, 1.1, 'a'), (2, 2.2, 'b')

SELECT * FROM tb1
1,1.1,a
2,2.2,b

SELECT * FROM tb1 WHERE id=1
1,1.1,a

UPDATE tb1 SET value=3.3 WHERE id=1

SELECT * FROM tb1 WHERE id=1
1,3.3,a

SELECT COUNT(*) FROM tb1
2

DELETE FROM tb1 WHERE id=1

SELECT * FROM tb1
2,2.2,b

DROP DATABASE test
